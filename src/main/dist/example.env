# Loads from remote config yaml file if set to production
# Loads from local eggbot.yaml file if not set to production
ENVIRONMENT=production
# URL to config
CONFIG_URL=
# Basic Auth username and password to access the config
CONFIG_USERNAME=
CONFIG_PASSWORD=
# PostgreSQL connection strings, one per server
DATABASE_URL=postgres://<username>:<password>@<host>:<port>/<database>

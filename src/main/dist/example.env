# Loads from remote config yaml file if set to production
# Loads from local eggbot.yaml file if not set to production
ENVIRONMENT=production
# URL to config
CONFIG_URL=
# Basic Auth username and password to access the config
CONFIG_USERNAME=
CONFIG_PASSWORD=
# For non-production: PostgreSQL JDBC connection string, one per server, configured in eggbot.yaml
DATABASE_JDBC_URL=jdbc:postgresql://<host>:<port>/<database>?user=<username>&password=<password>
